{
  "name": "@nx-example/components",
  "devDependencies": {
    "@types/node": "^17.0.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "tsup": "^5.11.7"
  },
  "scripts": {
    "build": "tsup --minify",
    "dev": "tsup --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts"
}
